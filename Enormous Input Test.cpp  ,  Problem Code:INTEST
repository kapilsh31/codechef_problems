
#include <bits/stdc++.h> 

using namespace std;
ios_base::sync_with_stdio(false);
	cin.tie(NULL);



int main() {
// 	ios_base::sync_with_stdio(false);
// 	cin.tie(NULL);


	int n, k,t;
	cin>> n >> k;

	
	int ans = 0,i=1;

	while( i <= n) {
		cin>>t;
		
		if (t % k == 0) {
			ans++;
		}	
		i++;
	}


	cout << ans << "\n";	
		
	return 0;
}
