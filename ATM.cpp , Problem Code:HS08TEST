#include <bits/stdc++.h>
using namespace std;

void atm(int x,float y){
    if(x+0.5 >y){
        cout<<fixed<<setprecision(2)<<y;
    }
    else if(x % 5 != 0){
        cout<<fixed<<setprecision(2)<<y;
    }
    else{
        y=y-x-0.5;
        cout<<fixed<<setprecision(2)<<y;
    }
}

int main() {
	int x; float y;
	cin>>x>>y;
	
	atm(x,y);
	return 0;
}
